{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "sag-sas/openseadragon-block",
	"version": "0.1.0",
	"title": "Openseadragon Imageviewer",
	"category": "media",
	"icon": "format-image",
	"description": "Zoomable image viewer using the open source OpenSeaDragon viewer with support for tiling images.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"imageUrl": {
			"type": "string"
		},
		"height": {
			"type": "string"
		},
		"useFeatured": {
			"type": "boolean",
			"default": true
		}
	},
	"textdomain": "openseadragon-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}
